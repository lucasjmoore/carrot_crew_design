<h1>Listing scrapers</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @scrapers.each do |scraper| %>
      <tr>
        <td><%= link_to 'Show', scraper %></td>
        <td><%= link_to 'Edit', edit_scraper_path(scraper) %></td>
        <td><%= link_to 'Destroy', scraper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scraper', new_scraper_path %>
